{
  "name": "elwins-test-web-components-vue",
  "sideEffects": false,
  "version": "0.3.2",
  "description": "Vue specific wrapper for elwins-test-web-components",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elwinvaneede/elwins-test-web-components.git"
  },
  "scripts": {
    "lint": "echo add linter",
    "test": "jest",
    "build": "npm run clean && npm run compile && npm run bundle",
    "bundle": "rollup --config rollup.config.js",
    "clean": "rimraf dist dist-transpiled",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@vue/test-utils": "^2.0.0-alpha.7",
    "rimraf": "^2.6.3",
    "rollup": "^2.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^3.9.7",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "elwins-test-web-components": "^0.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
