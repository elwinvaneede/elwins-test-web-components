{
  "name": "elwins-test-web-components",
  "version": "0.1.4",
  "description": "Some test Web Components build with Stencil",
  "repository": {
    "type": "git",
    "url": "https://github.com/elwinvaneede/elwins-test-web-components.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/elwins-test-web-components/elwins-test-web-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.15.0",
    "@stencil/react-output-target": "^0.0.6",
    "@stencil/vue-output-target": "0.0.1",
    "@types/jest": "^24.9.1",
    "@types/puppeteer": "^1.20.6",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "puppeteer": "^1.20.0"
  },
  "license": "MIT"
}
